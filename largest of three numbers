#include <stdio.h>
int main() {
int a,b,c;
printf{"enter the first number:"} ;
scanf{"%d", &a) ;
printf{"enter the second number:"} ;
scanf{"%d" , &b) ;
printf{"enter the third number:"} ;
scanf{"%d", &c) ;
if (a>b && a>c) {
printf("a is the largest") ;}
else if (b>c) {
printf("b is the largest"); }
else {
printf("c is the largest"); }
return 0 ;
}
